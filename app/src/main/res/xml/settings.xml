<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <CheckBoxPreference
        android:defaultValue="true"
        android:key="desc"
        android:summaryOff="@string/desc_off"
        android:summaryOn="@string/desc_on"
        android:title="@string/desc" />

    <ListPreference
        android:key="pref_list"
        android:entries="@array/strategy_names_user"
        android:entryValues="@array/strategy_names"
        android:summary="%s"
        android:title="@string/default_strategy_title" />
</PreferenceScreen>